extends layout

block head
  script(src='//ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js')
  script(src='/assets/js/app.js')

block content
  div.frame
    div.img-wrap
        img(src='#{imgurl}')
    div.siteurl
        a(href='#{siteurl}', target='_blank') #{siteurl}
  
  input(type='hidden', id='siteurl', value='#{siteurl}')
  input(type='hidden', id='imgurl', value='#{imgurl}')